{
  "name": "strangway",
  "version": "0.0.1",
  "description": "A todo list manager, note application, journal, and CRM.",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test ./node_modules/mocha/bin/mocha --recursive",
    "start": "NODE_ENV=dev nodemon index.js",
    "db:create": "./bin/create_databases.sh",
    "db:migrate:create": "./bin/create_migration.sh",
    "db:migrate": "for i in $(ls migrations/*.sql); do psql strangway_dev < $i; done;",
    "db:migrate:test": "for i in $(ls migrations/*.sql); do psql strangway_test < $i; done;",
    "db:migrate:pro": "for i in $(ls migrations/*.sql); do psql strangway < $i; done;"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/asommer70/strangway.git"
  },
  "keywords": [
    "notes",
    "todo",
    "markdown",
    "graphql"
  ],
  "author": "Adam Sommer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/asommer70/strangway/issues"
  },
  "homepage": "https://github.com/asommer70/strangway#readme",
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "cors": "^2.8.4",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "express-graphql": "^0.6.11",
    "express-session": "^1.15.6",
    "graphql": "^0.11.7",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg": "^7.3.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "mocha": "^4.0.1",
    "nodemon": "^1.12.1"
  }
}
